{
  "name": "eslint-plugin-codemuse",
  "version": "0.0.0",
  "description": "A comprehensive linting solution that sweeps your code clean",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "velenyx",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "/dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "init": "rm -rf dist node_modules && npm i",
    "build": "rm -rf ./dist && mkdir ./dist && cp -r ./lib/* ./dist",
    "pack": "npm pack --pack-destination=./dist",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "lint:syncpack": "syncpack list-mismatches",
    "lint:syncpack:fix": "syncpack fix-mismatches",
    "update:eslint-docs": "eslint-doc-generator",
    "commit": "cz",
    "release:prod": "git checkout main && git pull && npm run build && npm run pack && dotenv release-it -- -VV",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-node": "^11.1.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
